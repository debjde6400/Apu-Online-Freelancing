<main>
  <h1>
    Bid history
  </h1>

  <h2>
    For : <%= @user.name %>
  </h2>

  <div class="row">
    <% @bid_history.each do |b| %>
      <div class="col-lg-6">
        <%= render 'shared/bid_card', bid: b, project: b.project, user_wise: true %>
      </div>
    <% end %>
  </div>

  <div class="container text-center">
    <%= page_entries_info @bid_history %>
    <%= paginate @bid_history %>
  </div>
</main>

